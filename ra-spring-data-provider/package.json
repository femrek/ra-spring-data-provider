{
  "name": "ra-spring-data-provider",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "React Admin data provider for Spring Data REST APIs",
  "main": "./dist/ra-spring-data-provider.umd.js",
  "module": "./dist/ra-spring-data-provider.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ra-spring-data-provider.es.js",
      "require": "./dist/ra-spring-data-provider.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-admin",
    "data-provider",
    "spring-data",
    "spring-boot",
    "rest"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-admin": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "react": "^19.2.0",
    "react-admin": "^5.13.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
